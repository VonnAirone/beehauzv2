{
  "expo": {
    "name": "Beehauz",
    "slug": "beehauz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "analytics": {
      "enabled": true
    },
    "privacy": "app://privacy",
    "description": "Find your perfect student boarding house near Philippine universities. Search, compare, and connect with verified property owners.",
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to show boarding houses near your current location and the university.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.aironevonn.beehauz"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 24,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.aironevonn.beehauz",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAXn-flhdb__4Fqa-4pbPJsXk9DzF2ScPo"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-font",
      "expo-location",
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "MAPBOX_DOWNLOAD_TOKEN_PLACEHOLDER"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2dc26468-c0f9-43a8-a92b-bba64dce81b3"
      },
      "mapboxAccessToken": "pk.eyJ1IjoiZW5vcmlhbm92diIsImEiOiJjbWdjY3ZhcmIwMmtmMmtwdm1tbWRpMzA4In0._sY-Y_7ueIJ_gD0g90TYjQ",
      "adminDashboard": {
        "enabled": true,
        "tapSequenceCount": 7,
        "sessionDuration": 7200000,
        "maxAttempts": 3,
        "lockoutDuration": 900000
      }
    },
    "owner": "aironevonn"
  }
}
